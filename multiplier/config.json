{
    "name": "multiplier",
    "hyperparameters": [
        {
            "name": "WIDTH",
            "type": "integer",
            "default": "32"
        },
        {
            "name": "STAGE",
            "type": "integer",
            "default": "${WIDTH}"
        },
        {
            "name": "SIGNED",
            "type": "bit",
            "default": "1"
        },
        {
            "name": "OUTPUT_MSB",
            "type": "integer",
            "default": "2 * ${WIDTH} - 1"
        },
        {
            "name": "OUTPUT_LSB",
            "type": "integer",
            "default": "0"
        }
    ],
    "ports": [
        ["a", "${WIDTH}", "in"],
        ["b", "${WIDTH}", "in"],
        ["p", "${OUTPUT_MSB} - ${OUTPUT_LSB} + 1", "out"]
    ],
    "architectures": {
        "vivado_multiplier": {
            "includes": [
                {
                    "files": ["vivado_multiplier.vhd"]
                }
            ],
            "stages": "${STAGE}",
            "notice": "Remember to create a multiplier IP in Vivado named ip_vivado_multiplier_${__HYPERPARAMS_JOINED__} ..."
        }
    }
}